pipeline {
  agent {
    docker {
      image 'mcr.microsoft.com/playwright:v1.17.2-focal'
      label 'aws1'
    }
  }
  stages {
    stage('install playwright') {
      steps {
        sh '''
          npm i
        '''
      }
    }
    stage('test') {
      steps {
        sh 'npm run playwright test
      }
    }
  }
}